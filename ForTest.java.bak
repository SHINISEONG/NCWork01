//package jb02.part02;
public class ForTest
{
	public static void main(String[] args) 
	{
		int j = 5;
		System.out.println(j + "단을 출력합니다.");
		for (int i=1; i<10; i++)
		{
			System.out.println(j+" * "+i+ " = " + j*i);
		}
		int k=1;
		for(;;){
			if (k>10)
			{
				break;
			}
			System.out.println("여기는 for 무한루프");
		}

	}
}
