public class  Example2
{
	public static void main(String[] args) 
	{
		if (args.length == 1)
		{
			int gugudan = Integer.parseInt(args[0]);
			if (gugudan >= 2 && gugudan <=9)
			{
				System.out.println(gugudan + "단을 출력합니다.");
				System.out.println("==================================");
				for (int i=1; i<=9; i++)
				{
					System.out.println(gugudan+" x "+ i + " = " + (gugudan*i));
				}
			} else{
				System.out.println("구구단은 2단부터 9단까지만 존재합니다.");
			}
		} else {
			System.out.println("인수는 하나만 입력해 주세요.");
		}
	} //end of main
} // end of class
